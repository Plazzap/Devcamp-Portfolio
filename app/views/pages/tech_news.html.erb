<div class="inner cover">
  <h1 class="cover-heading">Rails News</h1>
  
  <div class="row">
    <% @tweets.each do |twitter| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <%= twitter_parser twitter%>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>